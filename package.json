{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "postinstall": "npm run prod",
        "setup": "composer install && php artisan key:generate && php artisan migrate && php artisan db:seed && php artisan storage:link && npm install && npm run dev",
        "resetdb": "php artisan migrate:refresh && php artisan db:seed"
    },
